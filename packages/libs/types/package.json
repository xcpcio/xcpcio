{
    "name": "@xcpcio/types",
    "description": "XCPCIO Types",
    "version": "0.3.2",
    "license": "MIT",
    "author": "Dup4 <lyuzhi.pan@gmail.com>",
    "keywords": [
        "ICPC",
        "CCPC"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xcpcio/xcpcio.git"
    },
    "homepage": "https://github.com/xcpcio/xcpcio",
    "bugs": {
        "url": "https://github.com/xcpcio/xcpcio/issues"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --stub",
        "start": "esmo src/index.ts",
        "test": "vitest",
        "lint": "run-p lint:*",
        "lint:build": "# tsc --noEmit"
    },
    "devDependencies": {
        "@babel/types": "^7.22.4",
        "@types/node": "^17.0.45",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "bumpp": "^7.2.0",
        "eslint": "^8.42.0",
        "esmo": "^0.14.1",
        "npm-run-all": "^4.1.5",
        "pnpm": "^7.33.0",
        "prettier": "^2.8.8",
        "taze": "^0.10.2",
        "typescript": "^4.9.5",
        "unbuild": "^0.7.6",
        "vite": "^4.3.9",
        "vitest": "^0.32.0"
    }
}
