{
  "name": "@xcpcio/core",
  "version": "0.40.1",
  "description": "XCPCIO Core",
  "author": "Dup4 <lyuzhi.pan@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/xcpcio/xcpcio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcpcio/xcpcio.git"
  },
  "bugs": {
    "url": "https://github.com/xcpcio/xcpcio/issues"
  },
  "keywords": [
    "ICPC",
    "CCPC"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "esmo src/index.ts",
    "test": "vitest",
    "lint": "run-p lint:*",
    "lint:build": "# tsc --noEmit"
  },
  "dependencies": {
    "@xcpcio/types": "workspace:*",
    "chroma-js": "^2.4.2",
    "color-diff": "^1.4.0",
    "dayjs": "^1.11.8",
    "lodash": "^4.17.21",
    "string-width": "^6.1.0",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.4",
    "@types/chroma-js": "^2.4.2",
    "@types/color-diff": "^1.2.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "bumpp": "^7.2.0",
    "eslint": "^8.42.0",
    "esmo": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.33.0",
    "taze": "^0.10.2",
    "typescript": "^4.9.5",
    "unbuild": "^0.7.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  }
}
