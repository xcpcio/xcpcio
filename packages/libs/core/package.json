{
  "name": "@xcpcio/core",
  "version": "0.58.0",
  "description": "XCPCIO Core",
  "author": "Dup4 <hi@dup4.com>",
  "license": "MIT",
  "homepage": "https://github.com/xcpcio/xcpcio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcpcio/xcpcio.git"
  },
  "bugs": {
    "url": "https://github.com/xcpcio/xcpcio/issues"
  },
  "keywords": [
    "ICPC",
    "CCPC"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "esmo src/index.ts",
    "test": "vitest",
    "lint": "run-p lint:*",
    "lint:build": "# tsc --noEmit"
  },
  "dependencies": {
    "@xcpcio/types": "workspace:*",
    "chroma-js": "catalog:",
    "color-diff": "catalog:",
    "dayjs": "catalog:",
    "js-base64": "catalog:",
    "lodash": "catalog:",
    "ordinal": "catalog:",
    "papaparse": "catalog:",
    "string-width": "catalog:",
    "xlsx-js-style": "catalog:"
  },
  "devDependencies": {
    "@babel/types": "catalog:",
    "@types/chroma-js": "catalog:",
    "@types/color-diff": "catalog:",
    "@types/lodash": "catalog:",
    "@types/node": "catalog:",
    "@types/papaparse": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "esmo": "catalog:",
    "npm-run-all": "catalog:",
    "pnpm": "catalog:",
    "taze": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
