{
  "name": "@xcpcio/xcpcio",
  "version": "0.20.0",
  "private": true,
  "packageManager": "pnpm@8.6.12",
  "description": "XCPCIO",
  "author": "Dup4 <lyuzhi.pan@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/xcpcio/xcpcio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcpcio/xcpcio.git"
  },
  "bugs": {
    "url": "https://github.com/xcpcio/xcpcio/issues"
  },
  "keywords": [
    "ICPC",
    "CCPC"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "pnpm -r --filter=./packages/**/* run build",
    "build:libs": "pnpm -r --filter=./packages/libs/**/* run build",
    "dev": "pnpm -r --parallel --filter=./packages/**/* run dev",
    "start": "pnpm -r --parallel --filter=./packages/**/* run start",
    "test": "vitest",
    "lint": "run-p lint:*",
    "lint:build": "pnpm -r --parallel --filter=./packages/**/* run lint:build",
    "lint:eslint": "eslint .",
    "format": "run-p format:*",
    "format:eslint": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json packages/**/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@babel/types": "^7.22.10",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bumpp": "^7.2.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "13.4.9",
    "esmo": "^0.14.1",
    "fsxx": "^0.0.5",
    "glob": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.12",
    "taze": "^0.10.3",
    "typescript": "^4.9.5",
    "unbuild": "^0.7.6",
    "unocss": "^0.53.6",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-beta.6",
    "vitest": "^0.32.4",
    "zx": "^6.2.5"
  }
}
